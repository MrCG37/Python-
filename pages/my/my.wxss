.container {
  font-family: Helvetica Neue, Helvetica, Arial, sans-serif;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  box-sizing: border-box;

}

.load {
  display: flex;
  flex-direction: column;
  padding: 25rpx;
  align-items: center;
  /* background: #35B6DD; */
}

.loadbutton {
  background-color: transparent;
  /* border-bottom: 5rpx solid #ffffff; */
  border-radius: 0;
}

.avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
  /* background: rgba(85, 106, 90, 0.676) */
}

.nickname {
  color: #fff;
}

.weui_cell {
  position: relative;
  display: flex;
  padding: 25px;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  border-bottom: 4rpx solid #dadada;
  background-color: white;
}

.weui_cell_hd {
  display: inline-block;
  width: 50rpx;
  margin-right: 5px;
}

.weui_cell_hd image {
  width: 100%;
  height: 25px;
  vertical-align: -4px;
}

.weui_cell_bd {
  display: inline-block;
}

.weui_cell_bd_t {
  font-size: 16px;
  color: #939393;
}

.advice {
  padding-top: 200rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 40rpx;
  color: #7a5e5ec0;
  /* border-bottom: 5rpx solid #BBBBBB; */
}

.background {
  width: 100%;
  height: 300rpx;
  position: fixed;
  /* background-size:100% 100%; */
  z-index: -1;
}